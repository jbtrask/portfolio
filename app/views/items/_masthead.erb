<div id="masthead"> 
	<div id="name">
		<h1>Jackie Goldberg</h1>
	</div> 
	<div id="nav">
		<%= render "nav" %>
	</div> 
	<div id="title">
		<% if @classification.nil? || @medium.nil? %>
    		<h3>Featured Work</h3>
		<% else %>
			<h3><%= @classification.name %></h3>
			<h3><%= @medium.name %></h3>
			<h3><%= @period.start_year.to_s + " - " + @period.end_year.to_s %></h3>
		<% end %>
	</div> 
	<div id="dimensions"></div>
</div>