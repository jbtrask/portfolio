@media only screen and (min-height: <%= @values.first[1][0] %>px) {
	.grid_cell {
		height: <%= 100.0 / @values.first[1][1][0].to_f %>%;
    min-height: <%= 100.0 / @values.first[1][1][0].to_f %>%;
	}
	#v-size:after {
		content: "vertical: <%= @values.first[0] %> -- <%= @values.first[1][1][0] %> -- <%= 100.0 / @values.first[1][1][0].to_f %>%"
	}
}

<% (1..@values.size - 2).each do |idx| %>
	@media only screen and (min-height: <%= @values[idx + 1][1][0] + 1 %>px) and (max-height: <%= @values[idx][1][0] %>px) {
		.grid_cell {
			height: <%= 100.0 / @values[idx][1][1][0] %>%;
      min-height: <%= 100.0 / @values[idx][1][1][0] %>%;
		}
		#v-size:after {
			content: "vertical: <%= @values[idx][0] %> -- <%= @values[idx][1][1][0] %> -- <%= 100.0 / @values[idx][1][1][0].to_f %>%"
		}
	}
<% end %>

@media only screen and (max-height: <%= @values.last[1][0] %>px) {
	.grid_cell {
		height: <%= 100.0 / @values.last[1][1][0].to_f %>%;
    min-height: <%= 100.0 / @values.last[1][1][0].to_f %>%;
	}
	#v-size:after {
		content: "vertical: <%= @values.last[0] %> -- <%= @values.last[1][1][0] %> -- <%= 100.0 / @values.last[1][1][0].to_f %>%"
	}
}


@media only screen and (min-width: <%= @values.first[1][0] %>px) {
	.grid_cell {
    width: <%= 100.0 / @values.first[1][1][1].to_f %>%;
		min-width: <%= 100.0 / @values.first[1][1][1].to_f %>%;
	}
	#h-size:after {
		content: "horizontal: <%= @values.first[0] %> -- <%= @values.first[1][1][1] %> -- <%= 100.0 / @values.first[1][1][1].to_f %>%"
	}
}

<% (1..@values.size - 2).each do |idx| %>
	@media only screen and (min-width: <%= @values[idx + 1][1][0] + 1 %>px) and (max-width: <%= @values[idx][1][0] %>px) {
		.grid_cell {
			width: <%= 100.0 / @values[idx][1][1][1] %>%;
      min-width: <%= 100.0 / @values[idx][1][1][1] %>%;
		}
		#h-size:after {
			content: "horizontal: <%= @values[idx][0] %> -- <%= @values[idx][1][1][1] %> -- <%= 100.0 / @values[idx][1][1][1].to_f %>%"
		}
	}
<% end %>

@media only screen and (max-width: <%= @values.last[1][0] %>px) {
	.grid_cell {
		width: <%= 100.0 / @values.last[1][1][1].to_f %>%;
    min-width: <%= 100.0 / @values.last[1][1][1].to_f %>%;
	}
	#h-size:after {
		content: "horizontal: <%= @values.last[0] %> -- <%= @values.last[1][1][1] %> -- <%= 100.0 / @values.last[1][1][1].to_f %>%"
	}
}
