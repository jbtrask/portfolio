}

#css_info {
	background-color: #800000;
	color: #FFFFFF;  
	border: 1px dotted #FFF;
	position: absolute;
	bottom: 0;
	right: 0;
	padding: 5px;
	margin: 10px;
	font-size: 10px;
	line-height: 14px;
	width: 68%;
}

#css_info hr {
	color: #888888;
	border-style: dotted;
}

#sidebar {
  background-color: #000000;
  overflow: hidden;
  position: absolute;
  top: 0;
  left: 0;
  width: 30%;
  min-width: 30%;
  height: 100%;
  min-height: 100%;
}

#main {
  background-color: #FFFFFF;
  overflow: hidden;
  position: absolute;
  display: table;
  top: 0;
  left: 30%;
  width: 70%;
  min-width: 70%;
  height: 100%;
  min-height: 100%;
}

#main ul {
	overflow: hidden;
  	margin: 0 auto;	
	width: 600px;
	min-width: 600px;
	height: 648px;
	min-height: 648px;
}

#cell {
	display: table-cell;
	width: 100%;
	height: 100%;
	min-width: 100%;
	min-height: 100%;
	vertical-align: middle;
}
.grid_cell {
	margin: 0;
  padding: 0;
  position: relative;
  float: left;
  width: 100px;
  min-width: 100px;
  height: 162px;
  min-height: 162px;
}

.grid_item {
  margin:  0
  padding: 0
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  min-width: 100%;
  height: 100%;
  min-height: 100%;
}

.grid_item.odd {
  background-color: #CCCCCC;
}

.grid_item.even {
  background-color: #DDDDDD;
}
